<div class="wrapper">
<h2>Make a Motto</h2>

<%= form_for [user, motto] do |f| %>
  <p><%= f.text_field :body, placeholder: "Create a motto for the week, this month, or life!", required: true, autocomplete: "off" %></p>
  <%= f.submit %>
<% end %>
<p class="return-link"><%= link_to "Return", user_path(user) %></p>
</div>
